<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Jost:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    <!-- Vendor CSS Files -->
    <link href="assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

    <link href="assets/vendor/aos/aos.css" rel="stylesheet">
    <link href="assets/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="assets/vendor/bootstrap-icons/bootstrap-icons.css" rel="stylesheet">
    <link href="assets/vendor/boxicons/css/boxicons.min.css" rel="stylesheet">
    <link href="assets/vendor/glightbox/css/glightbox.min.css" rel="stylesheet">
    <link href="assets/vendor/remixicon/remixicon.css" rel="stylesheet">
    <link href="assets/vendor/swiper/swiper-bundle.min.css" rel="stylesheet">

</head>

<body>

    <main id="main">



        <!-- ======= Contact Section ======= -->
        <section id="contact" class="contact">
            <div class="container">

                <div class="row">

                    <div class="col-lg-12 mt-5 mt-lg-0 d-flex align-items-stretch">
                        <form action="forms/contact.php" method="post" role="form" class="php-email-form">
                            <div class="row">
                                <div class="form-group col-md-4">
                                    <label for="name">Customer Name</label>
                                    <input type="text" name="name" class="form-control" id="name" value="<%=data[0].fullName%>">
                                </div>
                                <div class="form-group col-md-4">
                                    <label for="name">Customer Email</label>
                                    <input type="email" class="form-control" name="email" id="email" value="<%=data[0].emailId%>">
                                </div>
                                <div class="form-group col-md-4">
                                    <label for="name">Mobile No</label>
                                    <input type="text" class="form-control" name="Mobile" id="Mobile" value="<%=data[0].phoneNumber%>">
                                </div>
                            </div>


                            <div class="card">
                                <div class="card-body">
                                    <h5 class="card-title">Product Enquiry</h5>
                                    <!-- <p>Add <code>.table-bordered</code> for borders on all sides of the table and cells.</p> -->
                                    <!-- Bordered Table -->
                                    <table class="table table-bordered">
                                        <thead>
                                            <tr>
                                                <th scope="col">#</th>
                                                <th scope="col"> Product Name</th>
                                                <th scope="col">Product Code</th>
                                                <th scope="col">Quantity</th>
                                                <th scope="col">Color</th>
                                                <th scope="col">Size</th>
                                            </tr>
                                        </thead>
                                        <tbody>


                                            <% for(var i=0; i < data[0].enquiryMaster.enquiryProduct.length; i++) { %>
                                                <tr>
                                                    <td>

                                                    </td>

                                                    <td>
                                                        <%= data[0].enquiryMaster.enquiryProduct[i].product.productName %>
                                                    </td>
                                                    <td>
                                                        <%= data[0].enquiryMaster.enquiryProduct[i].product.code %>
                                                    </td>
                                                    <td>
                                                        <%= data[0].enquiryMaster.enquiryProduct[i].quantity %>
                                                    </td>
                                                    <td>
                                                        <%= data[0].enquiryMaster.enquiryProduct[i].color %>
                                                    </td>
                                                    <td>
                                                        <%= data[0].enquiryMaster.enquiryProduct[i].size %>
                                                    </td>
                                                </tr>
                                                <% } %>
                                        </tbody>
                                    </table>
                                    <!-- End Bordered Table -->

                                </div>
                            </div>
                            <div class="form-group">
                                <label for="name">  <strong>Notes :-</strong> </label>
                                <textarea class="form-control" name="message" rows="5"><%=data[0].notes%></textarea>
                            </div>
                        </form>
                    </div>

                </div>

            </div>
        </section>
        <!-- End Contact Section -->

    </main>
    <!-- End #main -->

    <!-- Vendor JS Files -->
    <script src="assets/vendor/aos/aos.js"></script>
    <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="assets/vendor/glightbox/js/glightbox.min.js"></script>
    <script src="assets/vendor/isotope-layout/isotope.pkgd.min.js"></script>
    <script src="assets/vendor/swiper/swiper-bundle.min.js"></script>
    <script src="assets/vendor/waypoints/noframework.waypoints.js"></script>
    <script src="assets/vendor/php-email-form/validate.js"></script>

    <!-- Template Main JS File -->
    <script src="assets/js/main.js"></script>
</body>

</html>